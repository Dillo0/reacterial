{
  "name": "reacterial",
  "version": "1.0.0",
  "description": "ReactJS (ES6) and Material Design app",
  "main": "bootstrapper.js",
  "private": "true",
  "scripts": {
    "start": "electron .",
    "sass": "sass --watch src/sass/master.scss:build/css/master.css --style compressed",
    "build": "electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --all --icon=icon.icns",
    "build-all": "electron-packager . --all --icon=icon.icns"
  },
  "repository": "none",
  "author": "Dani Vega @danivegamx - ingdanivega@gmail.com",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.1"
  },
  "devDependencies": {
    "electron": "^1.4.3",
    "electron-packager": "^8.1.0",
    "material-ui": "^0.16.0",
    "react-tap-event-plugin": "^2.0.1"
  }
}
